{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "coupons_and_deals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coupons-and-deal.coupons-and-deal",
      "mappedBy": "store"
    },
    "Social": {
      "type": "component",
      "repeatable": false,
      "component": "shared.social"
    },
    "Title": {
      "type": "text"
    },
    "Excerpt": {
      "type": "text"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "Favorit": {
      "type": "boolean"
    },
    "Rating": {
      "type": "integer",
      "max": 5,
      "min": 1
    },
    "Content": {
      "type": "blocks"
    }
  }
